<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="module.project.dao.PManagerMapper">
    <!--<cache flushInterval="300000" size="512" type="org.mybatis.caches.memcached.MemcachedCache"/>-->
    <resultMap id="GMemberMap" type="module.group.model.GMember">

        <id column="group_id" property="groupId"/>

        <id column="member_name" property="memberName"/>

        <result column="is_manager" property="isManager"/>

        <result column="create_time" property="createTime"/>

        <result column="update_time" property="updateTime"/>

    </resultMap>

    <insert id="insertGMember" >
        insert into group_member(group_id,member_name,is_manager,create_time,update_time)
        values (#{groupId},#{memberName},#{isManager},#{createTime},#{updateTime})
    </insert>

    <delete id="deleteGMember">
        delete from group_member where group_id=#{groupId} and member_name=#{memberName}
    </delete>



</mapper>